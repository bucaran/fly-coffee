{
  "name": "fly-coffee",
  "version": "0.1.0",
  "description": "CoffeeScript plugin for Fly",
  "license": "MIT",
  "repository": "https://github.com/bucaran/fly-coffee",
  "main": "index.js",
  "scripts": {
    "lint": "coffee *.js",
    "tape": "tape test/*.js",
    "test": "npm run lint && npm run tape | tspec",
    "build": "echo No build task specified.",
    "deploy": "npm run test && git push origin master && npm publish",
    "test-ci": "node --harmony --harmony_arrow_functions ./node_modules/tape/bin/tape test/*.js"
  },
  "author": "Jorge Bucaran",
  "dependencies": {
    "coffee-script": "^1.9.3"
  },
  "devDependencies": {
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0"
  },
  "engines": {
    "iojs": ">= 1.0.0",
    "node": ">= 0.11.0"
  }
}
